{% raw %}{%- from 'digitalocean_secret.sls' import digitalocean_public_key, digitalocean_private_key -%}{% endraw %}

master:
  firewall_blacklisted_ips: []
  ssh_keys:
    digitalocean_saltmaster:
      public: {% raw %}{{ digitalocean_public_key }}{% endraw %}
      private: |
          {% raw %}{{ digitalocean_private_key|indent(10) }}{% endraw %}

freeipa:
  ds_password: '{% raw %}{% include 'freeipa_ds_password.txt' %}{% endraw %}'
  admin_password: '{% raw %}{% include 'freeipa_admin_password.txt' %}{% endraw %}'

traefik:
  users:
    admin: '{% raw %}{% include 'traefik_admin_password.txt' %}{% endraw %}'
